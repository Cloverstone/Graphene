{{#categories}}
<div class="menu-catagory">
	<div>
		<h3>{{text}}</h3>
		<p>{{description}}</p>
	</div>
	<ul style="margin:0px">
		{{#items}}
		<li class="width6 pull-left filterable menu-item odd top1 bottom0">
			<div>
				<span class="name">{{name}}</span>
				<span class="price">{{price}}</span>
				<p>{{description}}</p>
			</div>
		</li>
		{{/items}}
	</ul>
</div>
{{/categories}}